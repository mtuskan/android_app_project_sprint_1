<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    >

    <fragment
        android:id="@+id/feedbackListFragment"
        android:name="edu.tacoma.uw.projectsprint1_group9.FeedbackListFragment"
        android:label="fragment_feedback_list"
        tools:layout="@layout/fragment_feedback_list" >
        <action
            android:id="@+id/action_feedbackListFragment_to_feedbackDetailFragment"
            app:destination="@id/feedbackDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/feedbackDetailFragment"
        android:name="edu.tacoma.uw.projectsprint1_group9.FeedbackDetailFragment"
        android:label="fragment_feedback_detail"
        tools:layout="@layout/fragment_feedback_detail">
        <argument
            android:name="feedback"
            app:argType="edu.tacoma.uw.projectsprint1_group9.Feedback" />
    </fragment>
    <fragment
        android:id="@+id/addFeedbacksFragment"
        android:name="edu.tacoma.uw.projectsprint1_group9.AddFeedbacksFragment"
        android:label="fragment_add_feedbacks"
        tools:layout="@layout/fragment_add_feedbacks" >
        <action
            android:id="@+id/action_addFeedbacksFragment_to_feedbackListFragment"
            app:destination="@id/feedbackListFragment" />
    </fragment>

</navigation>
